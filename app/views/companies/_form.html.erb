<%= simple_form_for(@company) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombre %>
    <%= f.input :razonsocial %>
    <%= f.input :cif %>
    <%= f.input :direccion %>
    <%= f.input :cp %>
    <%= f.input :telefono %>
    <%= f.input :logo %>
    <%= f.input :active%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
