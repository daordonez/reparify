<%= simple_form_for(@part) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombreComun_parte %>
    <%= f.input :precio_parte %>
    <%= f.association :order %>
    <%= f.association :deviseModel %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
