<p id="notice"><%= notice %></p>

<h1>Invoices</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Item</th>
      <th>Baseimponible prec</th>
      <th>Ivaimponilbe prec</th>
      <th>Importetotal prec</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @invoices.each do |invoice| %>
      <tr>
        <td><%= invoice.customer %></td>
        <td><%= invoice.items%></td>
        <td><%= invoice.baseimponible_prec %></td>
        <td><%= invoice.ivaimponilbe_prec %></td>
        <td><%= invoice.importetotal_prec %></td>
        <td><%= link_to 'Show', invoice %></td>
        <td><%= link_to 'Edit', edit_invoice_path(invoice) %></td>
        <td><%= link_to 'Destroy', invoice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Invoice', new_invoice_path %>
