<%= simple_form_for(@devise_model) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombrecomercial_modelo %>
    <%= f.association :manufacturer, label_method: :nombre_fabricante %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Crear Nuevo Modelo", :class => 'btn btn-success' %>
  </div>
<% end %>
