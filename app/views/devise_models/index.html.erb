<p id="notice"><%= notice %></p>

<h1>Devise Models</h1>

<table>
  <thead>
    <tr>
      <th>Nombrecomercial modelo</th>
      <th>Manufacturer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @devise_models.each do |devise_model| %>
      <tr>
        <td><%= devise_model.nombrecomercial_modelo %></td>
        <td><%= devise_model.manufacturer %></td>
        <td><%= link_to 'Show', devise_model %></td>
        <td><%= link_to 'Edit', edit_devise_model_path(devise_model) %></td>
        <td><%= link_to 'Destroy', devise_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Devise Model', new_devise_model_path %>
